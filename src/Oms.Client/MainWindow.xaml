<Window x:Class="Oms.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="Subscribe" Content="Subscribe" HorizontalAlignment="Left" Margin="110,33,0,0" VerticalAlignment="Top" Width="75" Click="Subscribe_Click"/>
        <Button x:Name="Unsubcribe" Content="Unsubcribe" HorizontalAlignment="Left" Margin="110,71,0,0" VerticalAlignment="Top" Width="75" Click="Unsubcribe_Click" />
        <ItemsControl HorizontalAlignment="Left" Height="100" Margin="52,185,0,0" VerticalAlignment="Top" Width="407" ItemsSource="{Binding Orders}">
           

        </ItemsControl>

    </Grid>
</Window>
